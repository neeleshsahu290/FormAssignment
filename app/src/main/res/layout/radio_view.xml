<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="MissingDefaultResource">

    <TextView
        app:layout_constraintStart_toStartOf="parent"

        android:id="@+id/fieldName"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginVertical="@dimen/nm_margin_vertical"
        android:layout_marginHorizontal="@dimen/nm_margin_horizontal"
        tools:text="Select vender"
        android:text="Select vender"
        android:textColor="@color/commomTextColor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <RadioGroup
        app:layout_constraintTop_toBottomOf="@id/fieldName"
        android:id="@+id/radioGroup"
        android:layout_marginVertical="@dimen/nm_margin_vertical"
        android:layout_marginHorizontal="@dimen/nm_margin_horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="25dp">
    </RadioGroup>




    </androidx.constraintlayout.widget.ConstraintLayout>


